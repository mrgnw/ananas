{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
			"allowJs": true,
			"checkJs": true,
			"esModuleInterop": true,
			"forceConsistentCasingInFileNames": true,
			"paths": {
					"$lib/*": ["./src/lib/*"]
			},
			"resolveJsonModule": true,
			"skipLibCheck": true,
			"sourceMap": true,
			"strict": true
	},
	"include": [
			"src/**/*"
	],
	"exclude": [
			"node_modules/*"
	]
}