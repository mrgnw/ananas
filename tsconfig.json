{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
			"allowJs": true,
			"checkJs": true,
			"esModuleInterop": true,
			"forceConsistentCasingInFileNames": true,
			"resolveJsonModule": true,
			"skipLibCheck": true,
			"sourceMap": true,
			"strict": true
	},
	"include": [
			"src/**/*",
            ".svelte-kit/ambient.d.ts"
	],
	"exclude": [
			"node_modules/*"
	]
}