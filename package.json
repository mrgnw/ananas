{
  "name": "translate-xcc",
  "version": "0.0.1",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250409.0",
    "@sveltejs/kit": "^2.20.4",
    "@sveltejs/vite-plugin-svelte": "^4.0.4",
    "autoprefixer": "^10.4.21",
    "drizzle-kit": "^0.30.6",
    "drizzle-orm": "^0.41.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.0",
    "postcss": "^8.5.3",
    "postcss-load-config": "^5.1.0",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "svelte-check": "^4.1.5",
    "svelte-typewriter": "^3.2.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "vite": "^5.4.17",
    "wrangler": "^4.9.1"
  },
  "private": true,
  "scripts": {
    "dev": "vite dev --host",
    "build": "vite build",
    "preview": "vite preview --host",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "update-language-data": "bun scripts/fetch-wikidata-language-data.js",
    "update-country-data": "bun scripts/fetch-wikidata-country-data.js",
    "update-data": "bun scripts/fetch-wikidata-language-data.js && bun scripts/fetch-wikidata-country-data.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate:local": "drizzle-kit migrate",
    "db:migrate:prod": "wrangler d1 migrations apply ananas-auth-dev --remote",
    "deploy": "bun run build && bun run db:migrate:prod && wrangler pages deploy"
  },
  "type": "module",
  "dependencies": {
    "@sveltejs/adapter-cloudflare": "^7.0.1",
    "animotion": "^0.1.2",
    "bits-ui": "0.22.0",
    "clsx": "^2.1.1",
    "cmdk-sv": "^0.0.18",
    "countries-list": "^3.1.1",
    "kit-monorepo": "sveltejs/kit",
    "lucide-svelte": "^0.344.0",
    "svelte": "^5.25.9",
    "svelte-dnd-action": "^0.9.60",
    "svelte-inspect-value": "^0.4.1",
    "svelte-radix": "^1.1.1",
    "svelte-sonner": "^0.3.28",
    "tailwind-merge": "^2.6.0",
    "tailwind-variants": "^0.2.1",
    "underscore": "^1.13.7"
  }
}